<h1>Student Form</h1>

<%= form_tag students_path do %>
<p><%= label_tag 'First Name: ' %><br>
    <%= text_field_tag :'student[first_name]' %></p>

<p><%= label_tag 'Last Name: ' %><br>
    <%= text_field_tag :'student[last_name]' %></p>

<%= submit_tag "Submit Student" %>
<% end %>

<%= params.inspect%>